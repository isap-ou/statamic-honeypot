{
  "name": "isapp/statamic-honeypot",
  "description": "A lightweight, no-CAPTCHA spam protection module that silently blocks bots without disrupting real users.",
  "license": "proprietary",
  "support": {
    "issues": "https://github.com/isap-ou/statamic-honeypot/issues",
    "source": "https://github.com/isap-ou/statamic-honeypot"
  },
  "authors": [
    {
      "name": "Andrii Trush",
      "email": "andrii.trush@isapp.be"
    }
  ],
  "autoload": {
    "psr-4": {
      "Isapp\\Honeypot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Isapp\\Honeypot\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "statamic/cms": "^5.0",
    "spatie/laravel-honeypot": "^4"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "laravel/pint": "^1"
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "extra": {
    "statamic": {
      "name": "Honeypot Spam Protection",
      "description": "A lightweight, no-CAPTCHA spam protection module that silently blocks bots without disrupting real users."
    },
    "laravel": {
      "providers": [
        "Isapp\\Honeypot\\ServiceProvider"
      ]
    }
  },
  "prefer-stable": true,
  "version": "1.0.0"
}
